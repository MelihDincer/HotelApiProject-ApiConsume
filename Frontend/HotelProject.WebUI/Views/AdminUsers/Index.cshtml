@model List<HotelProject.EntityLayer.Concrete.AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}

<div class="active-member">
    <div class="table-responsive">
        <table class="table table-xs mb-0">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Ad Soyad</th>
                    <th>Şehir</th>
                    <th>Durum</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    count++;
                    <tr>
                        <td>@count</td>
                        <td><img src="@item.ImageUrl" class=" rounded-circle mr-3" alt="">@item.Name @item.Surname</td>
                        <td>@item.City</td>
                        <td><i class="fa fa-circle-o text-success  mr-2"></i>Aktif</td>
                    </tr>
                }               
            </tbody>
        </table>
        <a href="/Register/Index/" class="btn mb-1 mt-2 btn-rounded btn-outline-info">Yeni Kullanıcı Kaydı</a>
    </div>
</div>


